{
  "extends": "@megp/tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "paths": {
      "@/store/*": ["./src/store/*"],
      "@/models/*": ["./src/models/*"],
      "@/shared/*": ["./src/app/_shared/*"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
