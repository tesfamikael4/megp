    "gitops": "sh ../../../.gitops/gitops.sh 'apps/server/marketplace-be' marketplace-be  marketplaceBe",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prebuild": "rimraf dist",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./node_modules/megp-shared-be/dist/src/typeorm/typeorm-config-helper.js",
    "migration:run": "pnpm run typeorm:cli migration:run",
    "migration:generate": "pnpm run build && pnpm run typeorm:cli migration:generate src/migrations/%npm_config_name%",
    "migration:create": "pnpm run typeorm:cli migration:create",
    "migration:revert": "pnpm run typeorm:cli migration:revert"
  },
